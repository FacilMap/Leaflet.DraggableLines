{
  "name": "leaflet-draggable-lines",
  "version": "1.1.0",
  "description": "A Leaflet plugin to add/move/remove points on routes, lines and polygons by drag&drop.",
  "main": "./dist/L.DraggableLines.js",
  "module": "./dist/L.DraggableLines.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/L.DraggableLines.mjs",
      "require": "./dist/L.DraggableLines.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FacilMap/Leaflet.DraggableLines.git"
  },
  "license": "BSD-2-Clause",
  "files": [
    "example.html",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "FORMAT=es vite build && FORMAT=umd vite build",
    "clean": "rimraf dist",
    "dev-server": "vite",
    "check-types": "tsc --noEmit",
    "test": "jest --env=jsdom"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/leaflet": "^1.7.9",
    "@types/node": "^17.0.24",
    "jest": "^27.5.1",
    "leaflet": "^1.7.1",
    "leaflet-highlightable-layers": "^1.2.0",
    "mini-svg-data-uri": "^1.4.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-css-injected-by-js": "^1.4.0",
    "vite-plugin-dts": "^1.0.5"
  },
  "peerDependencies": {
    "leaflet": "x"
  },
  "dependencies": {
    "leaflet-geometryutil": "^0.10.1"
  },
  "packageManager": "yarn@3.2.0"
}
