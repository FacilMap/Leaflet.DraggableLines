{
  "name": "leaflet-draggable-lines",
  "version": "1.2.1",
  "description": "A Leaflet plugin to add/move/remove points on routes, lines and polygons by drag&drop.",
  "main": "./dist/L.DraggableLines.js",
  "module": "./dist/L.DraggableLines.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/L.DraggableLines.mjs",
      "require": "./dist/L.DraggableLines.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FacilMap/Leaflet.DraggableLines.git"
  },
  "license": "BSD-2-Clause",
  "files": [
    "example.html",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "FORMAT=es vite build && FORMAT=umd vite build",
    "clean": "rimraf dist",
    "dev-server": "vite",
    "check-types": "tsc --noEmit",
    "test": "jest --env=jsdom"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "leaflet": "^1.9.4",
    "leaflet-highlightable-layers": "^2.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0"
  },
  "peerDependencies": {
    "leaflet": "x"
  },
  "dependencies": {
    "leaflet-geometryutil": "^0.10.3"
  },
  "packageManager": "yarn@4.0.2"
}
